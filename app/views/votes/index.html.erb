<h1>Listing votes</h1>

<table>
  <thead>
    <tr>
      <th>Coffee</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.coffee %></td>
        <td><%= vote.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote', new_vote_path %>
